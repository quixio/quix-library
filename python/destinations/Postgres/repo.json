{
  "Variables": [
    {
      "Name": "input",
      "Type": "EnvironmentVariable",
      "InputType": "InputTopic",
      "Description": "This is the input topic for demo data",
      "DefaultValue": "f1-data",
      "Required": true
    },
    {
      "Name": "PG_HOST",
      "Type": "EnvironmentVariable",
      "InputType": "InputTopic",
      "Description": "Host name of Postgres",
      "DefaultValue": "",
      "Required": true
    },
    {
      "Name": "PG_PORT",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "Port of Postgres",
      "DefaultValue": "5432",
      "Required": true
    },
    {
      "Name": "PG_USER",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "Username of Postgres",
      "DefaultValue": "",
      "Required": true
    },
    {
      "Name": "PG_PASSWORD",
      "Type": "EnvironmentVariable",
      "InputType": "HiddenText",
      "Description": "Password of Postgres",
      "DefaultValue": "",
      "Required": true
    },
    {
      "Name": "PG_DATABASE",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "Database name of Postgres",
      "DefaultValue": "",
      "Required": true
    }
  ],
  "EntryPoint": "build/dockerfile",
  "RunEntryPoint": "main.py",
  "DeploySettings": {
    "DeploymentType": "Service",
    "CpuMillicores": 200,
    "MemoryInMb": 200,
    "Replicas": 1,
    "ValidateConnection": true
  }
}
            "Required": true
        },
        {
            "Name": "kafka_topic",
            "Type": "EnvironmentVariable",
            "InputType": "FreeText",
            "Description": "Name of the Confluent Kafka topic to sink data to",
            "Required": true
        },
        {
            "Name" : "kafka_key",
            "Type" : "EnvironmentVariable",
            "InputType" : "HiddenText",
            "Description" : "Obtained from the Confluent Kafka portal",
            "DefaultValue" : "",
            "Required": true
        },
        {
            "Name" : "kafka_secret",
            "Type" : "EnvironmentVariable",
            "InputType" : "HiddenText",
            "Description" : "Obtained from the Confluent Kafka portal",
            "DefaultValue" : "",
            "Required": true
        },
        {
            "Name" : "kafka_broker_address",
            "Type" : "EnvironmentVariable",
            "InputType" : "FreeText",
            "Description" : "Obtained from the Confluent Kafka portal",
            "DefaultValue" : "",
            "Required": true
        }
    ],
    "DeploySettings":
    {
        "DeploymentType": "Service",
        "CpuMillicores": 200,
        "MemoryInMb": 200,
        "Replicas": 1,
        "PublicAccess": false,
        "ValidateConnection": true

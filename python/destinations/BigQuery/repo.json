{
  "Variables": [
    {
      "Name": "input",
      "Type": "EnvironmentVariable",
      "InputType": "InputTopic",
      "Description": "This is the input topic for demo data",
      "DefaultValue": "f1-data",
      "Required": true
    },
    {
      "Name": "PROJECT_ID",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "The BigQuery GCP Project ID",
      "DefaultValue": "",
      "Required": true
    },
    {
      "Name": "DATASET_ID",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "The target BigQuery dataset ID",
      "DefaultValue": "",
      "Required": true
    },
    {
      "Name": "DATASET_LOCATION",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "Location of BigQuery dataset",
      "DefaultValue": "US",
      "Required": true
    },
    {
      "Name": "SERVICE_KEY_PATH",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "The path to the service key of the BigQuery GCP project",
      "DefaultValue": "",
      "Required": true
    }
  ],
  "EntryPoint": "build/dockerfile",
  "RunEntryPoint": "main.py",
  "DeploySettings": {
    "DeploymentType": "Service",
    "CpuMillicores": 200,
    "MemoryInMb": 200,
    "Replicas": 1,
    "ValidateConnection": true
  }
}

{
  "Variables": [
    {
      "Name": "output_topic",
      "Type": "EnvironmentVariable",
      "InputType": "OutputTopic",
      "Description": "This is the output topic to write data to",
      "DefaultValue": "transaction-data",
      "Required": true
    },
    {
      "Name": "nb_customers",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "",
      "DefaultValue": "5",
      "Required": false
    },
    {
      "Name": "start_date",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "",
      "DefaultValue": "1990-01-01",
      "Required": false
    },
    {
      "Name": "end_date",
      "Type": "EnvironmentVariable",
      "InputType": "FreeText",
      "Description": "",
      "DefaultValue": "2020-01-01",
      "Required": false
    }
  ],
  "EntryPoint": "build/dockerfile",
  "RunEntryPoint": "datagen.py"
}